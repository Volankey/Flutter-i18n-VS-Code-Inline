{
  "all": true,
  "check-coverage": true,
  "reporter": ["html", "lcov", "text", "text-summary"],
  "include": [
    "out/extension.js",
    "out/core/**/*.js",
    "out/providers/**/*.js",
    "out/editors/**/*.js",
    "out/utils/**/*.js",
    "out/commands/**/*.js"
  ],
  "exclude": ["out/test/**", "**/*.test.js", "**/*.spec.js", "coverage/**", "node_modules/**"],
  "statements": 30,
  "branches": 60,
  "functions": 15,
  "lines": 30
}
